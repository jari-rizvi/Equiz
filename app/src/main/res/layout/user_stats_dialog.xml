        android:text="0%"
        android:textColor="@color/black"

        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/textView303"
        app:layout_constraintEnd_toEndOf="@+id/textView312"
        app:layout_constraintStart_toStartOf="@+id/textView312"
        app:layout_constraintTop_toTopOf="@+id/textView303" />


    <com.google.android.material.progressindicator.LinearProgressIndicator
        android:id="@+id/simpleProgressBar3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp"
        android:max="100"

        app:indicatorColor="#5986E4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/simpleProgressBar2"
        app:layout_constraintStart_toStartOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/textView303"
        app:layout_constraintVertical_bias="0"
