<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/consRoot"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorAccent">

        <ImageView
            android:id="@+id/btnback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.05"
            app:srcCompat="@drawable/arrow_left"
            app:tint="@color/black" />

        <ImageView
            android:id="@+id/btnback121"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.05" />

        <TextView
            android:id="@+id/textView"
            style="@style/popins_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quiz"
            android:textColor="@color/black"
            android:textSize="@dimen/_14ssp"
            app:layout_constraintBottom_toBottomOf="@+id/btnback"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btnback"
            app:layout_constraintVertical_bias="0.05" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/button_radius_corner"
            android:backgroundTint="@color/white"
            android:elevation="@dimen/_3sdp"
            app:layout_constraintBottom_toTopOf="@+id/textView4654"
            app:layout_constraintEnd_toEndOf="@+id/btnback121"
            app:layout_constraintStart_toStartOf="@+id/btnback"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintVertical_bias="0.7">

            <ImageView
                android:id="@+id/imgTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/arrow_left"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.05"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.05"
                app:tint="#CDCDCD" />

            <TextView
                android:id="@+id/textView465454"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12/20"
                android:textColor="#CDCDCD"
                android:textSize="@dimen/_13ssp"
                app:layout_constraintBottom_toBottomOf="@+id/imgTitle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.03"
                app:layout_constraintStart_toEndOf="@+id/imgTitle"
                app:layout_constraintTop_toTopOf="@+id/imgTitle" />

            <com.google.android.material.progressindicator.CircularProgressIndicator
                android:id="@+id/progressbar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:progress="50"
                app:indicatorColor="#C62E27"
                app:indicatorSize="@dimen/_45sdp"
                app:layout_constraintBottom_toBottomOf="@+id/textView46545454"
                app:layout_constraintEnd_toEndOf="@+id/textView46545454"
                app:layout_constraintStart_toStartOf="@+id/textView46545454"
                app:layout_constraintTop_toTopOf="@+id/textView46545454"
                app:trackThickness="@dimen/_3sdp" />

            <TextView
                android:id="@+id/textView46545454"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text="100"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView465454" />

            <TextView
                android:id="@+id/textView4654545454"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text="What year was the very first model of the iPhone released?"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="@dimen/_13ssp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/progressbar" />


            <androidx.appcompat.widget.AppCompatCheckedTextView
                android:id="@+id/txtLogin1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:background="@drawable/button_radius_corner_red"
                android:paddingVertical="@dimen/_10sdp"
                android:text="2007"
                android:textAlignment="center"
                android:textColor="#9F81CA"
                android:textSize="@dimen/_13ssp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView4654545454" />

            <androidx.appcompat.widget.AppCompatCheckedTextView
                android:id="@+id/txtLogin112"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@drawable/button_radius_corner_red"
                android:paddingVertical="@dimen/_10sdp"
                android:text="2005"
                android:textAlignment="center"
                android:textColor="#9F81CA"
                android:textSize="@dimen/_13ssp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="@+id/txtLogin1"
                app:layout_constraintStart_toStartOf="@+id/txtLogin1"
                app:layout_constraintTop_toBottomOf="@+id/txtLogin1" />

            <androidx.appcompat.widget.AppCompatCheckedTextView
                android:id="@+id/txtLogin11542"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@drawable/button_radius_corner_red"
                android:paddingVertical="@dimen/_10sdp"
                android:text="2008"
                android:textAlignment="center"
                android:textColor="#9F81CA"
                android:textSize="@dimen/_13ssp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="@+id/txtLogin1"
                app:layout_constraintStart_toStartOf="@+id/txtLogin1"
                app:layout_constraintTop_toBottomOf="@+id/txtLogin112" />

            <androidx.appcompat.widget.AppCompatCheckedTextView
                android:id="@+id/txtLogin1154542"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_30sdp"
                android:background="@drawable/button_radius_corner_red"
                android:paddingVertical="@dimen/_10sdp"
                android:text="2002"
                android:textAlignment="center"
                android:textColor="#9F81CA"
                android:textSize="@dimen/_13ssp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/txtLogin1"
                app:layout_constraintStart_toStartOf="@+id/txtLogin1"
                app:layout_constraintTop_toBottomOf="@+id/txtLogin11542" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <TextView
            android:id="@+id/textView4654"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/button_radius_green"
            android:backgroundTint="#9F81CA"
            android:layout_marginHorizontal="@dimen/_30sdp"
            android:textAlignment="center"
            android:paddingVertical="@dimen/_7sdp"
            android:text="Result"
            android:textColor="@color/white"
            android:textSize="@dimen/_16ssp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout9"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout9"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.95" />

        <!--        <TextView-->
        <!--            android:id="@+id/textView212"-->
        <!--            style="@style/popins_bold"-->
        <!--            android:layout_width="0dp"-->
        <!--            android:layout_height="wrap_content"-->
        <!--            app:layout_constraintStart_toStartOf="@+id/btnback"-->
        <!--            android:text="What year was the very first model of the iPhone released?"-->
        <!--            android:textColor="@color/black"-->
        <!--            android:textSize="@dimen/_14ssp"-->
        <!--            app:layout_constraintEnd_toEndOf="@+id/btnback121"-->
        <!--            app:layout_constraintTop_toBottomOf="@+id/textView"/>-->


    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>